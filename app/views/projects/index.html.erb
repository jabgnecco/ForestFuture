<p style="color: green"><%= notice %></p>
<h1>All Projects</h1>
<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
    <p>
      <%= link_to 'Show this Project', project %>
    </p>
  <% end %>
</div>

<%= link_to 'Create New Project', new_project_path if policy(Project).new? %>
